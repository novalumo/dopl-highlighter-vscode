{
  "name": "novalumo-dopl-syntax-highlight",
  "displayName": "DOPL Syntax Highlight",
  "description": "Syntax highlighting support for DOPL - Document-oriented Paired-value Language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "dopl",
        "aliases": [
          "dopl",
          "DOPL"
        ],
        "extensions": [
          ".dopl"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "dopl",
        "scopeName": "source.dopl",
        "path": "./syntaxes/dopl.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "dopl",
        "path": "./snippet/dopl.snippet.json"
      }
    ]
  }
}
